{
    "version": 2,
    "public": true,
    "routes": [
      {
        "src": "/brands/:id",
        "dest": "/brands/:id"
      },
      {
        "src": "/categories/:id",
        "dest": "/categories/:id"
      },
      {
        "src": "/details/:id",
        "dest": "/details/:id"
      }
    ],
    "builds": [
      {
        "src": "angular.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist/final-project" }
      }
    ]
  }
  